<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:shadui="clr-namespace:ShadUI.Controls;assembly=ShadUI">

    <Color x:Key="ForegroundColor">#18181B</Color>
    <Color x:Key="ForegroundLeadColor">#71717A</Color>

    <Color x:Key="BackgroundColor">Transparent</Color>
    <Color x:Key="MutedColor">#71717A</Color>
    <Color x:Key="BorderColor">#CECECE</Color>

    <Color x:Key="CardBackgroundColor">#F7F7F7</Color>
    <Color x:Key="SidebarBackgroundColor">#FAFAFA</Color>
    <Color x:Key="TitleBarBackgroundColor">#FAFAFA</Color>
    <Color x:Key="PopupBackgroundColor">#FAFAFA</Color>
    <Color x:Key="WindowButtonHoverColor">#0D09090B</Color>

    <Color x:Key="SwitchForegroundColor">#F2F2F2</Color>
    <Color x:Key="SwitchBackgroundColor">#C0C0C2</Color>

    <Color x:Key="PrimaryColor">#09090B</Color>
    <Color x:Key="PrimaryColor75">#BF09090B</Color>
    <Color x:Key="PrimaryColor50">#8009090B</Color>
    <Color x:Key="PrimaryColor10">#1909090B</Color>
    <Color x:Key="PrimaryForegroundColor">#FAFAFA</Color>

    <Color x:Key="SecondaryColor">#D9D9D9</Color>
    <Color x:Key="SecondaryColor75">#BFD9D9D9</Color>
    <Color x:Key="SecondaryColor50">#80D9D9D9</Color>
    <Color x:Key="SecondaryForegroundColor">#18181B</Color>

    <Color x:Key="DestructiveColor">#DC2626</Color>
    <Color x:Key="DestructiveColor75">#BFDC2626</Color>
    <Color x:Key="DestructiveColor50">#80DC2626</Color>
    <Color x:Key="DestructiveForegroundColor">#FAFAFA</Color>

    <Color x:Key="TabItemSelectedColor">#FFFFFF</Color>
    <Color x:Key="TabItemsBackgroundColor">#F0F0F0</Color>
    <Color x:Key="OutlineColor">#BFE0E0E0</Color>
    <Color x:Key="GhostColor">#E6E6E6</Color>
    <Color x:Key="GhostHoverColor">#F0F0F0</Color>

    <Color x:Key="BusyAreaOverlayColor">#F2F2F2</Color>
    <Color x:Key="DialogOverlayColor">#000000</Color>
    <Color x:Key="DialogBackgroundColor">#F2F2F2</Color>

    <Color x:Key="InfoColor">#0288D1</Color>
    <Color x:Key="InfoColor60">#990288D1</Color>
    <Color x:Key="InfoColor20">#330288D1</Color>
    <Color x:Key="InfoColor10">#190288D1</Color>
    <Color x:Key="InfoColor5">#0D0288D1</Color>
    <Color x:Key="SuccessColor">#2E7D32</Color>
    <Color x:Key="SuccessColor60">#992E7D32</Color>
    <Color x:Key="SuccessColor20">#332E7D32</Color>
    <Color x:Key="SuccessColor10">#192E7D32</Color>
    <Color x:Key="SuccessColor5">#0D2E7D32</Color>
    <Color x:Key="WarningColor">#ED6C02</Color>
    <Color x:Key="WarningColor60">#99ED6C02</Color>
    <Color x:Key="WarningColor20">#33ED6C02</Color>
    <Color x:Key="WarningColor10">#19ED6C02</Color>
    <Color x:Key="WarningColor5">#0DED6C02</Color>
    <Color x:Key="ErrorColor">#D32F2F</Color>
    <Color x:Key="ErrorColor60">#99D32F2F</Color>
    <Color x:Key="ErrorColor20">#33D32F2F</Color>
    <Color x:Key="ErrorColor10">#19D32F2F</Color>
    <Color x:Key="ErrorColor5">#0DD32F2F</Color>

    <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="avares://ShadUI/Controls/Card.axaml" />
    </ResourceDictionary.MergedDictionaries>

    <ControlTheme
        BasedOn="{StaticResource AcrylicCardControlTheme}"
        TargetType="{x:Type shadui:Card}"
        x:Key="{x:Type shadui:Card}" />
</ResourceDictionary>